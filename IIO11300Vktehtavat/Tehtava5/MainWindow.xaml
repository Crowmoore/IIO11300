<Window x:Class="Tehtava5.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tehtava5"
        mc:Ignorable="d"
        Title="Snake" Height="350" Width="300">
    <StackPanel Orientation="Vertical">
        <Button x:Name="btnStart" Content="New Game" Width="150" Height="30" Margin="0, 100, 0, 0" Click="btnStart_Click"></Button>
        <Button x:Name="btnHighscores" Content="Highscores" Width="150" Height="30" Margin="20" Click="btnScore_Click"></Button>
        <Button x:Name="btnQuit" Content="Quit" Width="150" Height="30" Click="btnQuit_Click"></Button>
    </StackPanel>
</Window>
